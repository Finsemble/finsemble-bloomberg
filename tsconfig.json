{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
		"typeRoots": [
			"node_modules/@types",
			"node_modules/@finsemble/finsemble-core/types",
		]
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": []
}